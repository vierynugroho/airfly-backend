{
  "name": "binarfinalproject",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node .",
    "dev": "nodemon .",
    "prepare": "husky",
    "lint": "eslint .",
    "format": "prettier --write .",
    "docs:generate": "node src/utils/generate.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:db": "npx prisma db push",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:seed": "node ./prisma/seeder.js",
    "prisma:studio": "npx prisma studio",
    "prisma:refresh": "npm run prisma:reset && npm run prisma:migrate && npx prisma generate && npm run prisma:seed",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint  --fix",
      "git add"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",

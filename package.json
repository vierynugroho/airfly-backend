    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "9.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  }

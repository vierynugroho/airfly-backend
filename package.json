    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "http": "^0.0.1-security",
    "imagekit": "^5.2.0",
    "jest": "^29.7.0",
    "jest-http-mock": "^1.1.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "otpauth": "^9.3.5",
    "postman-to-openapi": "^1.7.3",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "@sentry/cli": "^2.38.2",
    "@sentry/node": "^8.38.0",
    "@sentry/profiling-node": "^8.38.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
